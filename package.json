{
  "name": "defy",
  "version": "0.1.2",
  "description": "Small JavaScript library for data validation with flexibility in mind.",
  "engines": {
    "node": ">=6.10.2"
  },
  "main": "index.js",
  "jsnext:main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "node build.js",
    "lint": "node lint.js",
    "setupprecommithook": "mkdir -p .git/hooks && cp bin/git-hooks--pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "precommit": "npm run build && npm run lint && npm run test",
    "postversion": "git push && git push --tags && npm publish",
    "prepare": "npm run setupprecommithook && npm run lint && npm run build && npm run test",
    "preversion": "npm run build",
    "test": "node test.js",
    "version": "git add ."
  },
  "keywords": [
    "defy",
    "validation",
    "validate",
    "check",
    "form",
    "field",
    "input",
    "value"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrSlide/defy.git"
  },
  "author": {
    "name": "Luis Rodrigues",
    "url": "http://www.luisrodriguesweb.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrSlide/defy/issues"
  },
  "homepage": "https://github.com/MrSlide/defy#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^1.1.3",
    "coveralls": "^2.13.1",
    "isparta": "^4.0.0",
    "jasmine-core": "^2.6.1",
    "karma": "^1.6.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "phantomjs-prebuilt": "^2.1.14",
    "standard": "^10.0.2",
    "text-table": "^0.2.0"
  }
}
